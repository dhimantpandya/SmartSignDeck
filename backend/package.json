    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongodb": "^6.5.0",
    "mongoose": "8.2.2",
    "mongoose-query-parser": "^1.3.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.12",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
